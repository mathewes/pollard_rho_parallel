A=vector(100,i,i);
for(i=35,99, print1(vecextract(A,1<<i)))
for(i=20,25, print(vecextract(A,1<<100-2-1<<i-1<<(2*i)-1<<(3*i))))
vecextract(A,"2..4")
vecextract(A,"-2..-4")
vecextract(A,"^65")
vecextract(matid(3),"2..","..")
vecextract(matid(3),"-2..","-1..")
matsize(vecextract(matid(3), 0, 3))
matsize(vecextract(matid(3), [], 3))
matsize(vecextract(matid(6), 3, 0))

